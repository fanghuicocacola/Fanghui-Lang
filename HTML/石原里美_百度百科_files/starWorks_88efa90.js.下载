define("wiki-lemma:widget/lemma_content/configModule/star/starWorks/starWorks",function(r){var e=r("wiki-common:widget/lib/jquery/jquery"),s=r("wiki-lemma:widget/tools/marquee/marquee");!function(){var r=new s({viewport:e("#slider_works")}),a=e(".works .switchBtn"),i=e(".works .left"),o=e(".works .right"),t=4*Math.floor(r.picCount/4);e(".works").on("click",".switchBtn",function(){var s=e(this),l=s.hasClass("left");s.hasClass("disabled")||(r.curFrame+=l?-4:4,a.removeClass("disabled"),r.curFrame<=0&&(r.curFrame=0,i.addClass("disabled")),r.curFrame>=t&&(r.curFrame=t,o.addClass("disabled")),r.switchTo(r.curFrame))})}()});