define("wiki-lemma:widget/lemma_content/configModule/shushuo/shushuoTitle/shushuoTitle",function(i,t){var o=i("wiki-common:widget/lib/jquery/jquery"),n=i("wiki-common:widget/component/nslog/nslog"),e={portrait:10090306,interact:10090306,partner:10090307,impress:10090309};t.init=function(i){var t={init:function(i){this.$btn=o("#J-shushuo-"+i+"-btn"),this.$link=o("#J-"+i+"-partner-href-0"),this.$layer=o("#J-"+i+"-tooltip-wrapper"),this.$tooltip=o("#J-"+i+"-tooltip"),this.$btn&&this.bindEvent()},bindEvent:function(){var t=this,l=null;t.$btn.on("mouseover",function(){var i=.5*o(window).height(),n=o(this).offset().top-o(window).scrollTop();n>i?t.$tooltip.removeClass("invert"):t.$tooltip.removeClass("invert").addClass("invert"),t.$layer.removeClass("hide"),t.$tooltip.removeClass("hide"),l&&(clearTimeout(l),l=null)}).on("mouseleave",function(){l&&(clearTimeout(l),l=null),l=setTimeout(function(){t.$layer.addClass("hide"),t.$tooltip.addClass("hide")},200)}),t.$btn.on("click",function(){n(e[i])}),t.$link.on("click",function(){n(e.partner)})}};t.init(i)}});