define("wiki-lemma:widget/lemma_content/configModule/star/starRelation/starRelation",function(e){var a=e("wiki-common:widget/lib/jquery/jquery"),i=e("wiki-lemma:widget/tools/marquee/marquee");!function(){var e=new i({viewport:a("#slider_relations")}),r=a(".relations .switchBtn"),t=a(".relations .left"),s=a(".relations .right"),l=3*Math.floor(e.picCount/3);a(".relations").on("click",".switchBtn",function(){var i=a(this),o=i.hasClass("left");i.hasClass("disabled")||(e.curFrame+=o?-3:3,r.removeClass("disabled"),e.curFrame<=0&&(e.curFrame=0,t.addClass("disabled")),e.curFrame>=l&&(e.curFrame=l,s.addClass("disabled")),e.switchTo(e.curFrame))})}()});