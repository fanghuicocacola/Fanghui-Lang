function string compare(int a,int b)
{
    if(a==b)
    {
        return 't';
    }
    elif(a<b)
    {
        return 'xiao';
    }
    elif(a>b)
    {
        return 'da';
    }
}


function int func()
{
    int sum=0;
    int tmp=0;
    while(true)
    {
        tmp=std.input_int();
        if(tmp<0)
        {
            return sum;
        }
        sum+=tmp;
    }
}


program main
{
    std.cout(func());
    std.sleep(100000000);
}